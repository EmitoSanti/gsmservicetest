        "helmet": "^3.12.1",
        "jsonwebtoken": "^8.3.0",
        "lodash": "^4.17.15",
        "mongodb": "^3.4.1",
        "mongoose": "^5.8.4",
        "mongoose-aggregate-paginate": "^1.1.3",
        "mongoose-aggregate-paginate-v2": "^1.0.4",
        "mongoose-paginate": "^5.0.3",
        "morgan": "^1.9.1",
        "node-cache": "4.2.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "readline": "^1.3.0",
        "typescript": "^3.3.3",
        "winston": "2.4.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.149",
